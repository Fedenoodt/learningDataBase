*DROP DATABASE FOREIGNKNOWLEDGE*

*ALTER TABLE ONE
DROP ID_MUCH;*

CREATE DATABASE FOREIGNKNOWLEDGE;

CREATE TABLE ONE (ID_ONE INTEGER PRIMARY KEY NOT NULL AUTO_INCREMENT, ID_MUCH INTEGER);
CREATE TABLE MUCH (ID_MUCH INTEGER PRIMARY KEY NOT NULL AUTO_INCREMENT);

ALTER TABLE ONE
	ADD CONSTRAINT ID_MUCH
	FOREIGN KEY (ID_MUCH)
	REFERENCES MUCH(ID_MUCH);
