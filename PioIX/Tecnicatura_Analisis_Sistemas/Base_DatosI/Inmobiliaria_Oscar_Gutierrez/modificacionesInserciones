UPDATE PROPIEDADES SET ALQUILER = 1 WHERE ID_PROPIEDAD = 1;
UPDATE PROPIEDADES SET ALQUILER_TEMP = 0 WHERE ID_PROPIEDAD = 1;
UPDATE PROPIEDADES SET VENTA = 1 WHERE ID_PROPIEDAD = 1;

UPDATE PROPIEDADES SET ALQUILER = 0 WHERE ID_PROPIEDAD = 2;
UPDATE PROPIEDADES SET ALQUILER_TEMP = 0 WHERE ID_PROPIEDAD = 2;
UPDATE PROPIEDADES SET VENTA = 1 WHERE ID_PROPIEDAD = 2;

UPDATE PROPIEDADES SET ALQUILER = 1 WHERE ID_PROPIEDAD = 3;
UPDATE  PROPIEDADES SET ALQUILER_TEMP = 1 WHERE ID_PROPIEDAD = 3;
UPDATE PROPIEDADES SET VENTA = 0 WHERE ID_PROPIEDAD = 3;

UPDATE PROPIEDADES SET ALQUILER = 1 WHERE ID_PROPIEDAD = 4;
UPDATE PROPIEDADES SET ALQUILER_TEMP = 0 WHERE ID_PROPIEDAD = 4;
UPDATE PROPIEDADES SET VENTA = 0 WHERE ID_PROPIEDAD = 4;

UPDATE PROPIEDADES SET ALQUILER = 0 WHERE ID_PROPIEDAD = 5;
UPDATE PROPIEDADES SET ALQUILER_TEMP = 0 WHERE ID_PROPIEDAD = 5;
UPDATE PROPIEDADES SET VENTA = 1 WHERE ID_PROPIEDAD = 5;

